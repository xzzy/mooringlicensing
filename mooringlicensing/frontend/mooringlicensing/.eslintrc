{
  "extends": [
      "eslint:recommended",
      "plugin:vue/recommended",
      "prettier"
  ],
  "plugins": [
      "vue",
      "prettier"
  ],
  "rules": {
      "prettier/prettier": "error"
  },
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2021
  },
  "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jquery": true
  },
  "globals": {
      "moment": true,
      "swal": true,
      "bootstrap": true,
      "env": true,
      "_": true // Lodash
  }
}